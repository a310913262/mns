{
  "name": "mns-core",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:dev": "npx wrangler pages dev .vercel/output/static",
    "pages:deploy": "npm run pages:build && npx wrangler pages deploy .vercel/output/static --project-name mns-core --branch production",
    "pages:preview": "npm run pages:build && npx wrangler pages deploy .vercel/output/static --project-name mns-core --branch preview",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.561.0",
    "next": "15.4.10",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "reactflow": "^11.11.4",
    "tailwind-merge": "^3.4.0",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^9",
    "eslint-config-next": "15.4.10",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.0.0"
  }
}
